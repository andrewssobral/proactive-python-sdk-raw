<?xml version="1.0" encoding="UTF-8"?>
<job xmlns="urn:proactive:jobdescriptor:dev" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="urn:proactive:jobdescriptor:dev http://www.activeeon.com/public_content/schemas/proactive/jobdescriptor/dev/schedulerjob.xsd"
	 name="NativeWorkflow">
	<taskFlow>
		<task name="NativeTask">
			<nativeExecutable>
				<staticCommand value="myApplication">
					<arguments>
						<argument value="-arg"/>
						<argument value="oneArgument"/>
						<argument value="-otherArg"/>
						<argument value="anotherArgument"/>
					</arguments>
				</staticCommand>
			</nativeExecutable>
		</task>
	</taskFlow>
</job>